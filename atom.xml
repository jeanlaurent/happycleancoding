---
layout: nil
blogurl: http://happycleancoding.com
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>./happycleancoding</title>
	<link href="{{ blogurl }}/atom.xml" rel="self" />
	<link href="{{ blogurl }}/" />
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>{{ blogurl }}</id>
	<author>
		<name>Jean-Laurent de Morlhon</name>
		<email>jeanlaurent at morlhon.net</email>
	</author>
	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="{{ blogurl }}{{ post.url }}" />
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>{{ blogurl }}{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>